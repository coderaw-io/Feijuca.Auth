{
  "metadata": [
    {
      "src": [
        {
          "src": "../src/Feijuca.Keycloak.TokenManager/TokenManager.Api",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "styles/*.css",
          "sky.svg"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Feijuca.Keycloak",
      "_appTitle": "Feijuca.Keycloak",
      "_appLogoPath": "images/sky.svg",
      "_enableSearch": true,
      "pdf": true
    }
  },
  "styles":[
      "styles/main.css"
    ]
}